<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TitleBar">
        <attr name="leftwidth" format="dimension" />
        <attr name="rightwidth" format="dimension" />
        <attr name="lefttext" format="string" />
        <attr name="righttext" format="string" />
        <attr name="titletext" format="string" />
        <attr name="titletcolor" format="color" />
        <attr name="titlesize" format="dimension" />
        <attr name="bt_padding" format="dimension" />
        <attr name="bt_margin_top" format="dimension" />
        <attr name="bt_margin_bottom" format="dimension" />
        <attr name="bt_margin_left" format="dimension" />
        <attr name="bt_margin_right" format="dimension" />
        <attr name="leftTextColor" format="color" />
        <attr name="rightTextColor" format="color" />
        <attr name="leftbackground" format="reference" />
        <attr name="rightbackground" format="reference" />
    </declare-styleable>
    <declare-styleable name="EditTextDrawable">
        <attr name="lineColor" format="color" />
        <attr name="hint" format="string" />
        <attr name="text" format="string" />
        <attr name="hintColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="inputType">
            <enum name="text" value="1" />
            <enum name="number" value="2" />
            <enum name="number_password" value="18" />
            <enum name="password" value="129" />
            <enum name="phone" value="3" />
        </attr>
        <attr name="textsize" format="dimension" />
        <attr name="drawableWidth" format="dimension" />
        <attr name="drawableLeftPadding" format="dimension" />
        <attr name="drawableRightPadding" format="dimension" />
        <attr name="drawableLeft" format="reference" />
    </declare-styleable>
    <declare-styleable name="PercentProgressBar">
        <attr name="colors" format="color" />
        <attr name="restColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textVisibility" format="boolean" />
        <attr name="barHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
    <declare-styleable name="GifViews">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="GifTextureView">
        <attr name="gifSource" format="reference|string" />
        <attr name="isOpaque" format="boolean" />
    </declare-styleable>

</resources>